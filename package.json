{
  "name": "myftudp",
  "version": "1.0.0",
  "description": "myFTudP - Cliente/Servidor FTP com UDP e Interface Gr√°fica",
  "main": "src/client/main.js",
  "scripts": {
    "start": "electron .",
    "server": "node src/server/server.js",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "build": "electron-builder",
    "pack": "electron-builder --dir"
  },
  "keywords": ["ftp", "udp", "electron", "file-transfer"],
  "author": "Thalles Felipe",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "path": "^0.12.7",
    "fs-extra": "^11.2.0"
  },
  "build": {
    "appId": "com.student.myFTudP",
    "productName": "myFTudP",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "assets/**/*",
      "data/**/*",
      "node_modules/**/*"
    ]
  }
}
